1 REM COMPUTES FIRST BOOK OF 64 GAMES 
2 REM HIGH CARD
3 REM UPDATED BY JUSTIN BALDOCK
10 REM NECESSARY INITIAL INFORMATION
20 DOLLARS=500
30 PRINT " WITH WHOM DO I HAVE THE PLEASURE"
40 PRINT " OF PLAYING HIGH CARD SLICE?"
50 INPUT NAME$
60 PRINT " HIGH CARD WINS IN THIS GAME!"
70 DIM SUIT$(4), CARD$(14):FOR I=11 TO 14: READ CARD$(I):NEXT I
80 FOR I=1 TO 4: READ SUIT$(I):NEXT I
1000 REM
1001 REM MAIN PROGRAM LOOP
1010 PRINT:PRINT" YOU HAVE $" DOLLARS
1020 IF DOLLARS<=0 THEN PRINT" THE GAME IS OVER. YOU ARE OUT OF CASH.":END
1030 PRINT" WHAT IS YOUR BET";:INPUT BET
1040 IF DOLLARS<BET THEN PRINT " YOU ONLY HAVE $"DOLLARS" TO BET,"NAME$:GOTO 130
1040 YOURCARD=0:YURSUIT=0
1060 PLAYER$=NAME$
1070 GOSUB 3000
1080 YOURCARD=CARD:YURSUIT=SUIT
1090 PLAYER$="THE COMPUTER"
1100 GOSUB 3000
1110 IF CARD>YOURCARD THEN GOSUB 2000
1120 IF CARD=YOURCARD THEN GOSUB 2200
1130 IF CARD<YOURCARD THEN GOSUB 2500
1140 GOTO 1000:REM LOOP BACK TO START OF MAIN LOOP
2000 REM
2001 REM SUBROUTINE COMPUTER WINS
2010 PRINT " THE COMPUTER WINS"
2020 DOLLARS=DOLLARS-BET
2030 RETURN
2200 REM
2201 REM SUBROUTINE DRAW
2210 PRINT " A TIE!"
2220 RETURN
2500 REM
2501 REM SUBROUTINE PLAYER WINS
2510 PRINT " " NAME$ " WINS"
2520 DOLLARS = DOLLARS + BET
2530 RETURN
3000 REM 
3001 REM SUBROUTINE TO DRAW THE CARDS
3010 CARD=INT(RND(5)*13)+2:SUIT=INT(RND(5)*4)+1
3020 IF CARD=YOURCARD AND SUIT=YURSUIT THEN GOTO 3000:REM NO IDENTICAL DRAWS
3030 IF CARD<11 THEN CARD$(CARD)=STR$(CARD)
3040 PRINT " " PLAYER$ " DRAWS THE " CARD$(CARD) " OF " SUIT$(SUIT)
3050 RETURN
